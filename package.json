{
  "name": "yet-another-electron-term",
  "version": "0.1.0",
  "main": "src-electron/electronMain.js",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf dist",
    "build": "npm run clean && ng build --base-href ./ && cross-env NODE_ENV=production  electron-builder",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start": "concurrently \"npm run ng:serve\" \"npm run electron:dev\"",
    "ng:serve": "ng serve",
    "electron:dev": "wait-on http://localhost:4200 && cross-env NODE_ENV=development electron .",
    "postinstall": "patch-package"
  },
  "build": {
    "appId": "io.github.invince",
    "productName": "YetAnotherElectronTerm",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/",
      "src-electron/",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "src-electron/assets/icons/app-icon.ico"
    },
    "linux": {
      "icon": "src-electron/assets/icons/"
    },
    "mac": {
      "icon": "src-electron/assets/icons/app-icon.icns"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.12",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.12",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@novnc/novnc": "^1.5.0",
    "@syncfusion/ej2-angular-base": "^28.1.33",
    "@syncfusion/ej2-angular-filemanager": "^28.1.36",
    "@types/novnc__novnc": "^1.5.0",
    "@xterm/addon-web-links": "^0.11.0",
    "archiver": "^7.0.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "express": "^4.21.2",
    "find-free-ports": "^3.1.1",
    "keytar": "^7.9.0",
    "multer": "^1.4.5-lts.1",
    "ng-terminal": "^6.2.0",
    "ngx-spinner": "^17.0.0",
    "node-pty": "^1.0.0",
    "patch-package": "^8.0.0",
    "rxjs": "~7.8.0",
    "simple-git": "^3.27.0",
    "ssh2": "^1.16.0",
    "ssh2-sftp-client": "^11.0.0",
    "tslib": "^2.3.0",
    "uuid": "^11.0.3",
    "ws": "^8.18.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.11",
    "@angular/cli": "^18.2.11",
    "@angular/compiler-cli": "^18.2.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^9.1.1",
    "cross-env": "^7.0.3",
    "electron": "^31.7.5",
    "electron-builder": "^25.1.8",
    "electron-packager": "^17.1.2",
    "electron-rebuild": "^3.2.9",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.5.2",
    "wait-on": "^8.0.1"
  }
}
