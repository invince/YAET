{
  "name": "yet-another-electron-term",
  "version": "2.0.1",
  "main": "src-electron/electronMain.js",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf dist",
    "build": "npm run clean && patch-package && ng build --base-href ./ && electron-builder",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start": "concurrently \"npm run ng:serve\" \"npm run electron:dev\"",
    "ng:serve": "ng serve",
    "electron:dev": "wait-on http://localhost:4200 && cross-env NODE_ENV=development electron .",
    "postinstall": "patch-package",
    "rebuild-native": "electron-builder install-app-deps",
    "build:release": "npm run clean && patch-package && ng build --base-href ./ && electron-builder --publish always",
    "release": "npm version patch && npm run build:release"
  },
  "build": {
    "appId": "io.github.invince",
    "productName": "YetAnotherElectronTerm",
    "publish": {
      "provider": "github",
      "owner": "invince",
      "repo": "YAET-RELEASE"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "uninstallerIcon": "src-electron/assets/icons/uninstall-icon.ico",
      "warningsAsErrors": false,
      "include": "src-electron/custom-uninstall.nsh"
    },
    "files": [
      "dist/",
      "src-electron/",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "src-electron/assets/icons/app-icon.ico"
    },
    "linux": {
      "icon": "src-electron/assets/icons/"
    },
    "mac": {
      "icon": "src-electron/assets/icons/app-icon.icns"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.1",
    "@angular/cdk": "^21.0.1",
    "@angular/common": "^21.0.1",
    "@angular/compiler": "^21.0.1",
    "@angular/core": "^21.0.1",
    "@angular/forms": "^21.0.1",
    "@angular/material": "^21.0.1",
    "@angular/platform-browser": "^21.0.1",
    "@angular/platform-browser-dynamic": "^21.0.1",
    "@angular/router": "^21.0.1",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "@novnc/novnc": "^1.5.0",
    "@types/lodash": "^4.17.13",
    "@types/novnc__novnc": "^1.5.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "basic-ftp": "^5.0.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "electron-log": "^5.2.4",
    "electron-updater": "^6.3.9",
    "express": "^4.21.2",
    "find-free-ports": "^3.1.1",
    "https-proxy-agent": "^7.0.6",
    "keytar": "^7.9.0",
    "lodash": "^4.17.21",
    "multer": "^2.0.2",
    "ngx-spinner": "^17.0.0",
    "node-fstab": "^1.0.16",
    "node-powershell": "^5.0.1",
    "node-pty": "^1.0.0",
    "patch-package": "^8.0.0",
    "rxjs": "~7.8.0",
    "simple-git": "^3.27.0",
    "socks-proxy-agent": "^8.0.5",
    "ssh2": "^1.16.0",
    "ssh2-sftp-client": "^11.0.0",
    "telnet-client": "^2.2.2",
    "tslib": "^2.3.0",
    "uuid": "^11.0.3",
    "v9u-smb2": "^1.0.6",
    "ws": "^8.18.0",
    "xterm-addon-fit": "^0.8.0",
    "yazl": "^3.3.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.1",
    "@types/crypto-js": "^4.2.2",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^9.1.1",
    "cross-env": "^7.0.3",
    "electron": "^31.7.5",
    "electron-builder": "^25.1.8",
    "electron-packager": "^17.1.2",
    "electron-rebuild": "^3.2.9",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.9.3",
    "wait-on": "^8.0.1"
  }
}
